  form
  .container
   .projectinfo
    .col-md-5.col-sm-6.col-lg-4.col-xs-3    
     each d in data
      img.img-responsive(src=d.src, alt="")
    .col-md-3.col-sm-5.col-lg-4.col-xs-3#firstEdit
      .form-group
        input#editTitle.form-control(type='text', placeholder='Project title', name = 'product.title')
      .form-group
        input#editAuthor.form-control(type='text', placeholder='Author', name = 'product.author')
      .form-group
        input#editCategory.form-control(type='text', placeholder='Category', name='product.category') 
  .container       
   .col-md-8.col-sm-11.col-lg-8.col-xs-8#secondEdit
       .form-group
        input#editDescr.form-control(type='text', name='project.description', placeholder='Description')
       button.btn.btn-default#postbtn(type='submit') DONE      
 .foot1
  .container
   .col-lg-9.col-md-8.col-sm-8#low
    p#wrights COYWRIGHT 2013 DISPLAY ALL RIGHTS RESERVED
   .col-lg-3.col-md-4.col-sm-4#low
    .flinks
